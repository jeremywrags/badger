<!-- ======= Header ======= -->
<header id="header">
  <div class="logo float-left" style="background-color: #ffffff; padding: 5px; position:relative; top: -10px;">    
    <a href="/"><img src="/images/logos/Badger-Equipment-primary-logo-color.png" alt="" class="img-fluid"></a>
  </div>
  <div class="container">      
    <nav class="nav-menu float-left d-none d-lg-block">
      <ul>
        <li class="active"><a href="/"><i class="fa fa-home" aria-hidden="true"></i></a></li>
        <li><a caption="Interest In About Us" id="LinkAboutUs" href="#about">About Us</a></li>          
        <li class="drop-down"><a caption="Interest in Products" id="LinkProducts" href="#products">Products</a>
          <ul id="productMenu">
          <% categories.forEach(function(category) { %>            
            <li id="menuItem_<%= category.name.replace(/\s/g, '') %>" data-filter=".filter-<%= category.name.replace(/\s/g, '') %>">
              <a caption="Interest In <%= category.name %>" id="Link_Menu_<%= category.name.replace(/\s/g, '') %>" href="<%= pageURLref %>#products"><%= category.name %> </a>
            </li>  
          <% }); %>  
          </ul>
        </li>
        <li><a caption="Interest In Services" id="LinkServices" href="#services">Services</a></li>          
        <li><a caption="Interest In Our Team" id="LinkTeam" href="#team">Our Team</a></li>        
        <li><a caption="Interest In Contact Us" id="Contact" href="#contact">Contact Us</a></li>
      </ul>
    </nav><!-- .nav-menu -->
  </div>
</header><!-- End Header -->
