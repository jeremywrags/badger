                    <li id="InterestIn<%= category.name.replace(/\s/g, '') %>" data-filter=".filter-<%= category.name.replace(/\s/g, '') %>"><%= category.name %></li>
              <% }); %>  
              <!--
              <li id="InterestDriveTrain" data-filter=".filter-drivetrain">Drive Train</li>
              <li id="InterestEngineParts"data-filter=".filter-engineparts">Engine Parts</li>
              <li id="InterestNewItems"data-filter=".filter-new">Filters</li>
              <li id="InterestNewItems"data-filter=".filter-new">Hydraulics</li>
              <li id="InterestNewItems"data-filter=".filter-new">Seals</li>
              -->
            </ul>
          </div>
        </div>

        <div class="row portfolio-container">
            <% products.forEach(function(product) { %>                                            
              <div class="col-lg-4 col-md-6 portfolio-item filter-<%= product.family.replace(/\s/g, '')%>">
              <div class="portfolio-wrap">
               
                  <img src="<%=product.img%>" class="img-fluid" alt="">                
              
                <div class="portfolio-info">
                  <h4><%= product.name %></h4>
                  <p><%= product.description %></p>
                  <div class="portfolio-links">
                    <a href="<%=product.img%>" data-gall="portfolioGallery" class="venobox" title="Axle"><i class="icofont-eye"></i></a>
                    <a href="/productDetail/<%= product.id %>" title="More Details"><i class="icofont-external-link"></i></a>
                  </div>
                </div>
              </div>
            </div>
            <% }); %>                     


        </div>            
      </div>       
    </section><!-- End Our Portfolio Section -->

